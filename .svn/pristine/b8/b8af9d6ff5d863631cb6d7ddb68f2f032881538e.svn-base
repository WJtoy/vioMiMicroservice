package com.kkl.kklplus.b2b.viomi.utils;


import java.util.HashMap;
import java.util.Map;

public enum VioMIStatusCodeEnum {
    APPOINTMENT(3, "E0008","预约"),
    CANCEL(10, "E0009","服务取消"),
    FINISH(5, "E0010","服务完成");

    public int kklCode;
    public String viomiCode;
    public String name;
    private static final Map<Integer, VioMIStatusCodeEnum> MAP = new HashMap();

    private VioMIStatusCodeEnum(int kklCode, String viomiCode,String name) {
        this.kklCode = kklCode;
        this.viomiCode = viomiCode;
        this.name = name;
    }

    public static VioMIStatusCodeEnum get(int kklCode) {
        return valueOf(kklCode);
    }

    public static VioMIStatusCodeEnum valueOf(Integer kklCode) {
        VioMIStatusCodeEnum statusCode = null;
        if (kklCode != null) {
            statusCode = (VioMIStatusCodeEnum)MAP.get(kklCode);
        }

        return statusCode;
    }

    static {
        MAP.put(APPOINTMENT.kklCode, APPOINTMENT);
        MAP.put(CANCEL.kklCode, CANCEL);
        MAP.put(FINISH.kklCode, FINISH);
    }
}
